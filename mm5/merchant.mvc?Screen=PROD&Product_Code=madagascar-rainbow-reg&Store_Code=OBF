var im36 = new ImageMachine( '\x6d\x61\x64\x61\x67\x61\x73\x63\x61\x72\x2d\x72\x61\x69\x6e\x62\x6f\x77\x2d\x72\x65\x67', 0, '\x6d\x61\x69\x6e\x5f\x69\x6d\x61\x67\x65', '\x74\x68\x75\x6d\x62\x6e\x61\x69\x6c\x73', '\x63\x6c\x6f\x73\x65\x75\x70\x5f\x64\x69\x76', '\x63\x6c\x6f\x73\x65\x75\x70\x5f\x69\x6d\x61\x67\x65', '\x63\x6c\x6f\x73\x65\x75\x70\x5f\x63\x6c\x6f\x73\x65', '', '\x4e', 274, 274, 0, '\x4e', 750, 560, 1, 125, 125, 0, '\x67\x72\x61\x70\x68\x69\x63\x73\x2f\x30\x30\x30\x30\x30\x30\x30\x31\x2f\x6d\x61\x64\x61\x67\x61\x73\x63\x61\x72\x2d\x72\x61\x69\x6e\x62\x6f\x77\x32\x2e\x6a\x70\x67' );
